{
  "name": "evoex",
  "version": "1.0.0",
  "description": "Evoex - Asistente de IA para Excel con Azure OpenAI",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "dev-server": "webpack serve --mode development",
    "start": "office-addin-debugging start manifest.xml",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-addin-manifest validate manifest.xml",
    "lint": "eslint src --ext .ts,.tsx",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,html,css}\"",
    "generate-icons": "node scripts/generate-icons.js"
  },
  "dependencies": {
    "@microsoft/office-js": "^1.1.83"
  },
  "devDependencies": {
    "@types/office-js": "^1.0.377",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.0",
    "eslint": "^8.50.0",
    "html-webpack-plugin": "^5.5.3",
    "office-addin-debugging": "^5.0.0",
    "office-addin-dev-certs": "^1.11.0",
    "office-addin-manifest": "^1.12.0",
    "prettier": "^3.0.3",
    "semver": "^7.7.3",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "ChevyplanIT",
  "license": "MIT"
}
